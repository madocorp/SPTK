Window {
  width: 640px;
  height: 480px;
  x: 30%;
  y: 30%;
  backgroundColor: #000000;
  color: #ffff00;
  fontSize: 30px;
}

MenuBox2 {
  position: absolute;
  width: calculated;
  backgroundColor: #00aaaa;
  border: 3px;
  borderColor: #00ffff;
  padding: 0px;
  lineHeight: 100%;
  maxHeight: 60%;
  scrollable: true;
}

MenuBoxItem2 {
  width: 100%;
  padding: 2px, 0px, 0px, 0px;
  backgroundColor: inherit;
  color: #ffffff;
  height: 22px;
  textWrap: manual;
}
