Window {
  width: 640px;
  height: 480px;
  x: 30%;
  y: 30%;
  backgroundColor: #000000;
  color: #ffff00;
  lineHeight: 20px;
  padding: 10px;
}

.i {
  display: inline;
  width: 50px;
  height: 20px;
  backgroundColor: #ffffff;
  color: black;
  border: 3px;
  borderColor: #000000;
  font-size: 10px;
}

.b1 {
  display: block;
  width: 30px;
  height: 30px;
  backgroundColor: #ff0000;
  x: 5px;
  y: -5px;
}

.b2 {
  display: block;
  width: 30px;
  height: 30px;
  backgroundColor: #00ff00;
  x: 50%;
  y: -5px;
}

.b3 {
  display: block;
  width: 30px;
  height: 30px;
  backgroundColor: #0000ff;
  x: -5px;
  y: -5px;
}