Root {
  width: content;
  height: content;
  display: true;
  position: inline;
  x: 0px;
  y: 0px;
  border: 0px;
  borderColor: #000000;
  margin: 0px;
  padding: 0px;
  backgroundColor: inherit;
  backgroundImage: none;
  backgroundAlignX: center;
  backgroundAlignY: center;
  backgroundSizeX: content;
  backgroundSizeY: content;
  font: inherit;
  fontSize: 16px;
  color: inherit;
  textAlign: left;
  ascent: auto;
  lineHeight: 150%;
  letterSpacing: 1px;
  wordSpacing: 10px;
  textWrap: auto;
}

Window {
  width: 640px;
  height: 480px;
  backgroundColor: #0000aa;
  fontSize: 16px;
  font: LiberationMono-Bold;
}

Word {
  position: word;
  fontSize: inherit;
}

Box {
  position: absolute;
}

NL {
  position: newline;
  width: 0px;
  height: 0px;
}

Menu {
  position: absolute;
  width: 100%;
  height: 100%;
  x: 0px;
  y: 0px;
  backgroundColor: #00000000;
  padding: 0px;
  margin: 0px;
}

SubMenu {
  position: absolute;
  y: 20px;
}

MenuBar {
  position: absolute;
  width: 100%;
  height: 20px;
  backgroundColor: #00aaaa;
  color: #000000;
}

MenuBarHotKey {
  color: #0000ff;
  width: 20px;
  textAlign: right;
  paddingRight: 3px;
}

MenuBarItem {
  width: content;
  color: #000000;
  backgroundColor: inherit;
  padding: 1px, 15px, 0px, 0px;
}

MenuBarItem:active {
  backgroundColor: #00ffff;
}

MenuBox {
  position: absolute;
  width: 150px;
  height: content;
  backgroundColor: #00aaaa;
  border: 3px;
  borderColor: #00ffff;
  padding: 0px;
  lineHeight: 100%;
}

MenuBoxItem {
  width: 100%;
  padding: 2px, 0px, 0px, 0px;
  backgroundColor: inherit;
  color: #ffffff;
  height: 22px;
}

MenuBoxItemRight {
  position: absolute;
  x: -2px;
  y: 0px;
  color: #0000ff;
}

MenuBoxItemLeft {
  width: 15px;
  color: #0000ff;
  textAlign: center;
}

MenuBoxItem:active {
  backgroundColor: #000000;
}

.MenuSeparator {
  height: 25px;
  borderBottom: 3px;
  borderColor: #00ffff;
}

Panel {
  position: absolute;
  x: 50%;
  y: 50%;
  marginTop: half;
  marginLeft: half;
  width: 60%;
  height: content;
  backgroundColor: #cccccc;
  color: #000000;
  border: 3px;
  borderColor: #555555;
}

PanelTitle {
  width: 100%;
  height: 25px;
  backgroundColor: #555555;
  color: #ffffff;
  textAlign: center;
  paddingTop: 2px;
}

PanelContent {
  y: 17px;
  x: 0px;
  width: 100%;
  padding: 10px;
  color: #000000;
}

WarningPanel {
  position: absolute;
  x: 50%;
  y: 50%;
  marginTop: half;
  marginLeft: half;
  width: 60%;
  height: content;
  backgroundColor: #ffff00;
  color: #000000;
  border: 3px;
  borderColor: #ffaa00;
}

WarningPanelTitle {
  width: 100%;
  height: 25px;
  backgroundColor: #ffaa00;
  color: #ffffff;
  textAlign: center;
  paddingTop: 2px;
}

WarningPanelContent {
  y: 17px;
  x: 0px;
  width: 100%;
  padding: 10px;
  color: #000000;
}

ErrorPanel {
  position: absolute;
  x: 50%;
  y: 50%;
  marginTop: half;
  marginLeft: half;
  width: 60%;
  height: content;
  backgroundColor: #ffaaaa;
  color: #000000;
  border: 3px;
  borderColor: #cc0000;
}

ErrorPanelTitle {
  width: 100%;
  height: 25px;
  backgroundColor: #cc0000;
  color: #ffffff;
  textAlign: center;
  paddingTop: 2px;
}

ErrorPanelContent {
  y: 17px;
  x: 0px;
  width: 100%;
  padding: 10px;
  color: #000000;
}

Label {
  width: 100%;
  paddingBottom: 10px;
  lineHeight: 100%;
}

Input {
  width: 100%;
  backgroundColor: #00aaaa;
  color: #444444;
  height: 20px;
}

Input:active {
  color: #000000;
  backgroundColor: #00cccc;
}

InputValue {
  position: inline;
}

InputValue:selected {
  backgroundColor: #000000;
  color: #00cccc;
}

HalfBoxLeft {
  width: 50%;
  paddingRight: 9px;
  borderRight: 1px;
  borderColor: #555555;
}

HalfBoxRight {
  width: 50%;
  paddingLeft: 10px;
}

ButtonBox {
  width: 100%;
  marginTop: 10px;
  paddingTop: 10px;
  borderTop: 1px;
  borderColor: #555555;
  textAlign: justify;
}

Button {
  margin: 5px, 10px;
  color: #cccccc;
  backgroundColor: #555555;
  padding: 5px, 20px, 3px, 20px;
  border: 1px;
  borderColor: #aaaaaa;
}

Button:active {
  backgroundColor: #000000;
  color: #ffffff;
  borderColor: #ffffff;
}

ButtonHotKey {
  color: #00aaaa;
  paddingRight: 5px;
}
